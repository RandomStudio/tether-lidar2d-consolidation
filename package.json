{
  "version": "0.5.11",
  "name": "lidar-consolidation-agent",
  "description": "NodeJS Tether agent that receives and consolidates data from lidar agents",
  "author": "",
  "license": "ISC",
  "main": "dist/index.js",
  "scripts": {
    "postinstall": "npm run import-protos",
    "clean": "rm -rf node_modules dist",
    "build": "npm run import-protos && npx tsc && npm run copy-dist-files && npm run build-ui",
    "build-ui": "webpack --progress --profile --colors",
    "copy-dist-files": "shx cp package.json dist && shx cp .npmrc dist && shx cp -r src/messageClasses dist",
    "publish-agent": "npm publish --registry https://registry.tether-io.dev",
    "build-docker-image": "cross-var npm run build && docker build -t gitlab.random.nu:5566/tether/agents/$npm_package_name:$npm_package_version .",
    "push-docker-image": "npm run build-docker-image && cross-var docker push gitlab.random.nu:5566/tether/agents/$npm_package_name:$npm_package_version",
    "format": "prettier \"**/*.{ts,tsx,js,jsx,css,scss,sass,less,md}\" --write",
    "dev": "tsc && node dist/index.js --loglevel debug",
    "import-protos": "npx tether-protos-fetch && npm run compile-protos",
    "compile-protos": "rm -rf src/messageClasses && npx tether-protos-generate",
    "publish-protos": "npx tether-protos-publish"
  },
  "dependencies": {
    "body-parser": "^1.19.0",
    "classnames": "^2.2.6",
    "color-convert": "^2.0.1",
    "density-clustering": "^1.3.0",
    "express": "^4.17.1",
    "http": "0.0.1-security",
    "http-terminator": "^2.0.3",
    "log4js": "^6.1.0",
    "parse-strings-in-object": "^1.2.0",
    "rc": "^1.2.8",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "redux": "^4.0.5",
    "redux-subscribe-action": "^2.1.3",
    "rxjs": "^6.6.0",
    "tether-agent": "0.x",
    "ws": "^7.4.3"
  },
  "devDependencies": {
    "@babel/core": "^7.10.5",
    "@babel/plugin-transform-runtime": "^7.12.15",
    "@babel/preset-env": "^7.10.4",
    "@babel/preset-react": "^7.10.4",
    "@types/express": "^4.17.7",
    "@types/google-protobuf": "^3.7.2",
    "@types/node": "^12.12.8",
    "@types/rc": "^1.1.0",
    "@types/redux": "^3.6.0",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^8.1.0",
    "copy-webpack-plugin": "^6.0.3",
    "cross-var": "^1.1.0",
    "css-loader": "^4.0.0",
    "eslint": "^7.5.0",
    "eslint-config-airbnb": "^18.2.0",
    "eslint-loader": "^4.0.2",
    "eslint-plugin-babel": "^5.3.1",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-jsx-a11y": "^6.3.1",
    "eslint-plugin-react": "^7.20.4",
    "file-loader": "^6.0.0",
    "html-webpack-plugin": "^4.3.0",
    "mini-css-extract-plugin": "^0.9.0",
    "node-sass": "^4.14.1",
    "nodemon": "^2.0.1",
    "postcss-loader": "^3.0.0",
    "prettier": "^1.19.1",
    "prop-types": "^15.7.2",
    "sass-loader": "^9.0.2",
    "shx": "^0.3.2",
    "style-loader": "^1.2.1",
    "tether-protos-fetch": "0.x",
    "tether-protos-generate": "0.x",
    "tether-protos-publish": "0.x",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^3.7.3",
    "url-loader": "^4.1.0",
    "webpack": "^4.44.0",
    "webpack-cli": "^3.3.12"
  },
  "bin": {
    "lidar-consolidation-agent": "bin/outside"
  }
}
